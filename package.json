{
    "name": "@ionaru/array-utils",
    "type": "module",
    "version": "8.0.1",
    "description": "A number of utility functions for working with arrays.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "clean": "npx -q rimraf dist",
        "prebuild": "npm run clean",
        "build": "tsc --project tsconfig.json",
        "lint": "eslint --max-warnings 0 src",
        "lint:src": "eslint --ext ts --max-warnings 0 --ignore-pattern *.spec.ts src",
        "lint:test": "eslint --ext ts --plugin jest src/**/*.spec.ts",
        "pretest": "npm run lint",
        "test": "npx vitest run src --coverage",
        "prepare": "npm run build",
        "preversion": "npm run test"
    },
    "keywords": [],
    "author": "Jeroen Akkerman",
    "repository": "github:Ionaru/array-utils",
    "license": "MIT",
    "devDependencies": {
        "@ionaru/eslint-config": "^10.0.1-59.0",
        "@types/node": "^20.2.5",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "@vitest/coverage-v8": "^0.32.0",
        "eslint": "^8.42.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-sonarjs": "^0.19.0",
        "eslint-plugin-unicorn": "^47.0.0",
        "eslint-plugin-vitest": "^0.2.5",
        "typescript": "~5.0.0",
        "vitest": "^0.32.0"
    }
}
