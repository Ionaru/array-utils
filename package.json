{
    "name": "@ionaru/array-utils",
    "version": "8.0.1",
    "description": "A number of utility functions for working with arrays.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "clean": "npx -q rimraf dist",
        "prebuild": "npm run clean",
        "build": "tsc --project tsconfig.json",
        "lint": "npm run lint:src && npm run lint:test",
        "lint:src": "eslint --ext ts --max-warnings 0 --ignore-pattern *.spec.ts src",
        "lint:test": "eslint --ext ts --plugin jest --env jest/globals src/**/*.spec.ts",
        "pretest": "npm run lint",
        "test": "jest src",
        "prepare": "npm run build",
        "preversion": "npm run test"
    },
    "jest": {
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!src/**/*.d.ts",
            "!src/**/*.spec.ts",
            "!src/__mocks__/*.ts"
        ],
        "preset": "ts-jest",
        "testEnvironment": "node"
    },
    "keywords": [],
    "author": "Jeroen Akkerman",
    "repository": "github:Ionaru/array-utils",
    "license": "MIT",
    "devDependencies": {
        "@ionaru/eslint-config": "^10.0.0",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.2.5",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "eslint": "^8.42.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-sonarjs": "^0.19.0",
        "eslint-plugin-unicorn": "^47.0.0",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.0",
        "typescript": "^5.1.3"
    }
}
