{
    "name": "@ionaru/array-utils",
    "version": "6.2.0",
    "description": "A number of utility functions for working with arrays.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "clean": "npx -q rimraf dist",
        "build": "npm run clean && tsc --project tsconfig.json",
        "lint": "npm run lint:src && npm run lint:test",
        "lint:src": "eslint --ext ts --max-warnings 0 --ignore-pattern *.spec.ts src",
        "lint:test": "eslint --ext ts --plugin jest --env jest/globals src/**/*.spec.ts",
        "pretest": "npm run lint",
        "test": "jest src",
        "prepare": "npm run build",
        "preversion": "npm run test"
    },
    "jest": {
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!src/**/*.d.ts",
            "!src/**/*.spec.ts",
            "!src/__mocks__/*.ts"
        ],
        "preset": "ts-jest",
        "testEnvironment": "node"
    },
    "keywords": [],
    "author": "Jeroen Akkerman",
    "repository": "github:Ionaru/array-utils",
    "license": "MIT",
    "devDependencies": {
        "@ionaru/eslint-config": "^6.2.0",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.10.3",
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "eslint": "^8.9.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jest": "^26.1.1",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-sonarjs": "^0.12.0",
        "eslint-plugin-unicorn": "^41.0.0",
        "jest": "^27.2.5",
        "ts-jest": "^27.0.5",
        "typescript": "^4.4.3"
    }
}
